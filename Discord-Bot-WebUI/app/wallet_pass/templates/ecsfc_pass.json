{
  "formatVersion": 1,
  "passTypeIdentifier": "pass.com.ecsfc.membership.v3",
  "teamIdentifier": "{{team_identifier}}",
  "organizationName": "ECS FC",
  "serialNumber": "ecsfc-v2-{{uuid}}",
  "description": "ECS Pub League Membership",
  "barcode": {
    "format": "PKBarcodeFormatQR",
    "message": "{{barcode_data}}",
    "messageEncoding": "utf-8"
  },
  "generic": {
    "primaryFields": [
      {
        "key": "name",
        "label": "Name",
        "value": "{{user_name}}"
      }
    ],
    "secondaryFields": [
      {
        "key": "team",
        "label": "Team",
        "value": "{{team_name}}"
      },
      {
        "key": "season",
        "label": "Season",
        "value": "{{season}}"
      }
    ],
    "auxiliaryFields": [
      {
        "key": "player_id",
        "label": "Player ID",
        "value": "{{player_id}}"
      },
      {
        "key": "status",
        "label": "Status",
        "value": "{{status}}"
      }
    ],
    "backFields": [
      {
        "key": "league",
        "label": "League",
        "value": "{{league_name}}"
      },
      {
        "key": "phone", 
        "label": "Contact",
        "value": "{{phone}}"
      },
      {
        "key": "season_history",
        "label": "Season History",
        "value": "{{season_history}}"
      },
      {
        "key": "issued",
        "label": "Issued",
        "value": "{{issue_date}}"
      },
      {
        "key": "qr_purpose",
        "label": "QR Code Info",
        "value": "Present this pass for match check-in, team verification, and membership validation."
      }
    ]
  },
  "backgroundColor": "#213e96",
  "foregroundColor": "#ffffff",
  "labelColor": "#c8c8c8",
  "logoText": "ECS FC",
  "webServiceURL": "{{web_service_url}}",
  "authenticationToken": "{{auth_token}}",
  "expirationDate": "{{expiration_date}}",
  "voided": {{voided}}
}