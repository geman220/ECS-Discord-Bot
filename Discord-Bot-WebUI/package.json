{
  "name": "ecs-discord-bot-webui",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build:tailwind": "tailwindcss -i ./app/static/css/tailwind-entry.css -o ./app/static/vite-dist/css/tailwind.css --minify",
    "build:vite": "NODE_OPTIONS='--max-old-space-size=2048' vite build",
    "build": "npm run build:vite && npm run build:tailwind",
    "preview": "vite preview",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint app/static/js app/static/custom_js app/static/assets/js",
    "lint:fix": "eslint app/static/js app/static/custom_js app/static/assets/js --fix",
    "audit": "node scripts/audit-all.cjs",
    "audit:js": "node scripts/audit-js-patterns.cjs",
    "audit:css": "node scripts/audit-css-patterns.cjs",
    "audit:css-duplicates": "node scripts/audit-css-duplicates.cjs",
    "audit:tdz": "node scripts/audit-tdz-globals.cjs"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.11",
    "autoprefixer": "^10.4.23",
    "cropperjs": "^1.6.0",
    "datatables.net": "^2.1.0",
    "datatables.net-dt": "^2.3.6",
    "datatables.net-responsive": "^3.0.0",
    "datatables.net-responsive-dt": "^3.0.0",
    "feather-icons": "^4.29.0",
    "flatpickr": "^4.6.13",
    "flowbite": "^4.0.1",
    "hammerjs": "^2.0.8",
    "jquery": "^3.7.1",
    "node-waves": "^0.7.6",
    "perfect-scrollbar": "^1.5.5",
    "postcss": "^8.5.6",
    "shepherd.js": "^11.2.0",
    "socket.io-client": "^4.7.0",
    "sortablejs": "^1.15.0",
    "sweetalert2": "^11.14.0",
    "tailwindcss": "^3.4.19",
    "tinymce": "^8.3.2"
  },
  "devDependencies": {
    "@rollup/plugin-inject": "^5.0.5",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@vitest/coverage-v8": "^4.0.17",
    "css": "^3.0.0",
    "eslint": "^9.39.2",
    "glob": "^10.5.0",
    "happy-dom": "^20.1.0",
    "madge": "^8.0.0",
    "vite": "^6.0.0",
    "vitest": "^4.0.17"
  }
}
